isbn = {9781467318396},
issn = {10502947},
journal = {Phys. Rev. A - At. Mol. Opt. Phys.},
number = {6},
pages = {1--9},
title = {{How to implement decoy-state quantum key distribution for a satellite uplink with 50-dB channel loss}},
volume = {84},
year = {2011}
}
